
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}
  
model Book {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  imageUrl    String? 
  title       String
  author      String
  categories  String[]
  description String
  rating      Float?
}